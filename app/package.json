{
  "name": "nuxt-init",
  "version": "1.0.0",
  "description": "Nuxt init",
  "author": "Brad Inscoe",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "dtos": "cd shared && typescript-ref && tsc -m ES6 dtos.ts"
  },
  "dependencies": {
    "@nuxtjs/auth": "^4.5.3",
    "@nuxtjs/axios": "^5.3.0",
    "@nuxtjs/dotenv": "^1.3.0",
    "@nuxtjs/vuetify": "^0.4.3",
    "@servicestack/client": "^1.0.15",
    "cross-env": "^5.2.0",
    "koa": "^2.5.2",
    "koa-router": "^7.4.0",
    "nuxt": "^2.15.8"
  },
  "devDependencies": {
    "@nuxtjs/proxy": "^1.3.1",
    "nodemon": "^2.0.1",
    "typescript": "^3.2.2"
  }
}
